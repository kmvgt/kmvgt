[32m+[m[32mclass PackagesController extends BaseController {[m
[32m+[m[41m    [m
[32m+[m[32m    protected $layout = 'master';[m
[32m+[m[41m    [m
[32m+[m[32m    public function packages()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->data['cycles'] = Cycle::all();[m
[32m+[m[41m        [m
[32m+[m[32m        $this->data['services'] = Service::orderBy('name')->get();[m
[32m+[m[41m       [m
[32m+[m[32m        $this->data['packages'] = Package::paginate(25);[m
[32m+[m[41m        [m
[32m+[m[32m        return $this->layout->content = View::make('packages',$this->data);[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[32m    public function packages_post(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $cycle_id = Input::get('cycle_id');[m
[32m+[m[32m        $service_id = Input::get('service_id');[m
[32m+[m[32m        $price = Input::get('price');[m
[32m+[m[41m        [m
[32m+[m[32m        if(!Package::exists($service_id,$cycle_id)){[m
[32m+[m[41m            [m
[32m+[m[32m            if(!empty($price) || is_numeric($price))[m
[32m+[m[32m            {[m
[32m+[m[32m                $package = new Package;[m
[32m+[m[32m                $package->service_id = $service_id;[m
[32m+[m[32m                $package->cycle_id = $cycle_id;[m
[32m+[m[32m                $package->price = $price;[m
[32m+[m[32m                $package->save();[m
[32m+[m[32m                return Redirect::to('packages')->with('success','Package has been successfully saved.');[m
[32m+[m[41m                   [m
[32m+[m[32m            }else{[m
[32m+[m[32m                return Redirect::to('packages')->with('error','Price must be a number.');[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m        }else{[m
[32m+[m[32m            return Redirect::to('packages')->with('error','Package already exists.');[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[32m    public function update_post(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        if(Request::ajax())[m
[32m+[m[32m        {[m
[32m+[m[32m            $id = Input::get('id');[m
[32m+[m[32m            $service = Input::get('service');[m
[32m+[m[32m            $cycle = Input::get('cycle');[m
[32m+[m[32m            $price = Input::get('price');[m
[32m+[m[32m            $package = Package::find($id);[m
[32m+[m[32m            $package->service_id = $service;[m
[32m+[m[32m            $package->cycle_id = $cycle;[m
[32m+[m[32m            $package->price = number_format($price,2);[m
[32m+[m[32m            $package->save();[m[41m [m
[32m+[m[32m            echo 1;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    public function delete(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        if(Request::ajax())[m
[32m+[m[32m        {[m
[32m+[m[32m            $package_id = Input::get('package_id');[m
[32m+[m[32m            $package = Package::find($package_id);[m
[32m+[m[32m            if(!empty($package) && Package::has_associated_members($package_id) == false){[m
[32m+[m[32m                $package->delete();[m
[32m+[m[32m                echo 1;[m
[32m+[m[32m            }else{[m
[32m+[m[32m                echo "Package cannot be deleted.";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function expires()[m
[32m+[m[32m    {[m
[32m+[m[32m         $this->data['packages'] = Memberpackage::whereRaw("UNIX_TIMESTAMP(expiration) <= ".strtotime('NOW'))->orderBy('id','desc')->paginate(50);[m
[32m+[m[32m         return $this->layout->content = View::make('packages_expires',$this->data);[m
[32m+[m[32m    }[m
