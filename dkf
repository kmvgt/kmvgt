[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mclass BaseController extends Controller {[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Setup the layout used by the controller.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return void[m
[32m+[m	[32m */[m
[32m+[m[32m    public $data = array();[m
[32m+[m[41m    [m
[32m+[m[32m    public function __construct()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (Sentry::check())[m
[32m+[m[32m        {[m
[32m+[m[32m            // User is not logged in, or is not activated[m
[32m+[m[32m             $this->data['admin'] = Sentry::getUser();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m	[32mprotected function setupLayout()[m
[32m+[m	[32m{[m
[32m+[m		[32mif ( ! is_null($this->layout))[m
[32m+[m		[32m{[m
[32m+[m			[32m$this->layout = View::make($this->layout);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
