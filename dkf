[32m+[m[32m<script type="text/javascript">[m
[32m+[m[32m    jQuery(document).ready(function(){[m
[32m+[m[32m        jQuery('#show-create-form').click(function(){[m
[32m+[m[32m             jQuery('#add-package-form').toggle();[m
[32m+[m[32m        });[m
[32m+[m[32m        jQuery('.package-update').click(function(){[m
[32m+[m[32m            jQuery(this).hide();[m
[32m+[m[32m            jQuery(this).parents('tr').find('span').hide();[m
[32m+[m[32m            jQuery(this).parents('tr').find('input[type="text"],select').show();[m
[32m+[m[32m            jQuery(this).parents('tr').find('.package-save').show();[m
[32m+[m[32m            // SET SELECT VALUES[m
[32m+[m[32m            jQuery(this).parents('tr').find('.package-service').val(jQuery(this).attr('data-service'));[m
[32m+[m[32m            jQuery(this).parents('tr').find('.package-cycle').val(jQuery(this).attr('data-cycle'));[m
[32m+[m[32m            jQuery(this).parents('tr').find('.package-price').val(jQuery(this).attr('data-price'));[m
[32m+[m[32m        });[m
[32m+[m[32m        jQuery('.package-save').click(function(){[m
[32m+[m[41m            [m
[32m+[m[32m            var id = jQuery(this).parents('tr').find('.package-id').val();[m
[32m+[m[32m            var service = jQuery(this).parents('tr').find('.package-service').val();[m
[32m+[m[32m            var cycle = jQuery(this).parents('tr').find('.package-cycle').val();[m
[32m+[m[32m            var price = jQuery(this).parents('tr').find('.package-price').val();[m
[32m+[m[41m            [m
[32m+[m[32m            jQuery.ajax({[m
[32m+[m[32m                url: '/packages/update',[m
[32m+[m[32m                type: 'POST',[m
[32m+[m[32m                data: 'id='+id+'&service='+service+'&cycle='+cycle+'&price='+price,[m
[32m+[m[32m                success:function(data){[m
[32m+[m[32m                    window.location.reload(true);[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[41m            [m
[32m+[m[32m            //jQuery(this).hide();[m
[32m+[m[32m            //jQuery(this).parents('tr').find('span').show();[m
[32m+[m[32m            //jQuery(this).parents('tr').find('input[type="text"],select').hide();[m
[32m+[m[32m            //jQuery(this).parents('tr').find('.package-update').show();[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m        jQuery('.delete-package').click(function(){[m
[32m+[m[32m            confirm = window.confirm("Are you sure you want to delete this item?");[m
[32m+[m[32m            if(confirm){[m
[32m+[m[32m                var package_id = jQuery(this).attr('data-id');[m
[32m+[m[32m                jQuery.ajax({[m
[32m+[m[32m                   type:'POST',[m
[32m+[m[32m                   url:'{{ URL::to('packages/delete') }}',[m
[32m+[m[32m                   data: 'package_id='+package_id,[m
[32m+[m[32m                   success:function(data){[m
[32m+[m[32m                        if(data == 1){[m
[32m+[m[32m                            window.location.reload(true);[m
[32m+[m[32m                        }else{[m
[32m+[m[32m                            console.log(data);[m
[32m+[m[32m                        }[m
[32m+[m[32m                   }[m[41m [m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[41m        [m
[32m+[m[32m    });[m
