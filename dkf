[32m+[m[41m   [m
[32m+[m[32m   /**[m
[32m+[m[32m    * @return Gets the value of setting by name[m
[32m+[m[32m    */[m
[32m+[m[32m   public static function value($name = '')[m
[32m+[m[32m   {[m
[32m+[m[32m        $setting = Setting::where('name','=',$name)->first();[m
[32m+[m[32m        if(is_object($setting))[m
[32m+[m[32m        {[m
[32m+[m[32m            return $setting->value;[m
[32m+[m[32m        }else { return false; }[m
[32m+[m[32m   }[m
[32m+[m[32m   /**[m
[32m+[m[32m    * @return string rule[m
[32m+[m[32m    */[m
[32m+[m[32m    public static function rule($name = '')[m
[32m+[m[32m    {[m
[32m+[m[32m        $setting = Setting::where('name','=',$name)->first();[m
[32m+[m[32m        if(is_object($setting[0]))[m
[32m+[m[32m        {[m
[32m+[m[32m            return $setting->rule;[m
[32m+[m[32m        }else { return false; }[m
[32m+[m[32m    }[m
[32m+[m[41m   [m
[32m+[m[41m   [m
[32m+[m[32m}[m
