[32m+[m[32m<?php[m
[32m+[m[32mclass ActivitiesController extends BaseController {[m
[32m+[m[41m    [m
[32m+[m[32m    protected $layout = 'master';[m
[32m+[m[41m    [m
[32m+[m[32m    public function activities(){[m
