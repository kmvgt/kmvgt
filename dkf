[32m+[m[32m                                @if($p->status == 1)[m
[32m+[m[32m                                    <span class="label label-success">Active</span>[m
[32m+[m[32m                                @else[m
[32m+[m[32m                                    <span class="label label-danger">Inactive</span>[m
[32m+[m[32m                                @endif[m
[32m+[m[32m                            </td>[m
[32m+[m[32m                            <td>[m
[32m+[m[32m                                @if($p->status == 1)[m
[32m+[m[32m                                    <a class="btn btn-primary suspend" title="Suspend" data-id="{{ $p->id }}"><i class="glyphicon glyphicon-pause"></i></a>[m
[32m+[m[32m                                @else[m
[32m+[m[32m                                    <a class="btn btn-primary suspend" title="Suspend" data-id="{{ $p->id }}"><i class="glyphicon glyphicon-play"></i></a>[m
[32m+[m[32m                                @endif[m
[32m+[m[32m                                <a class="btn btn-success extend" title="Extend" data-id="{{ $p->id }}"><i class="glyphicon glyphicon-time"></i></a>[m
[32m+[m[32m                                <a class="btn btn-danger delete" title="Delete" data-id="{{ $p->id }}"><i class="glyphicon glyphicon-trash"></i></a>[m
[32m+[m[32m                            </td>[m
[32m+[m[32m                        </tr>[m
[32m+[m[32m                        @endif[m
[32m+[m[32m                   @endforeach[m
[32m+[m[32m                </tbody>[m
[32m+[m[32m            </table>[m
[32m+[m[32m            <div style="float: none; margin:auto; text-align:center;">[m
[32m+[m[32m            <script type="text/javascript">[m
[32m+[m[32m                jQuery(document).ready(function(){[m
[32m+[m[32m                    jQuery('div.pagination > ul').each(function onEachPaginator (i, item) {[m
[32m+[m[32m                        jQuery(item).addClass('pagination').parent().removeClass('pagination');[m
[32m+[m[32m                    });[m
[32m+[m[32m                });[m
[32m+[m[32m            </script>[m
