[32m+[m[32m    }[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @return Checks if the member has this package then return the package object if exists. Return false if not[m
[32m+[m[32m     */[m
[32m+[m[32m    public static function still_has($member_id,$package_id)[m
[32m+[m[32m    {[m
[32m+[m[32m        $package = Memberpackage::where('member_id','=',$member_id)->where('package_id','=',$package_id);[m
[32m+[m[32m        if($package->count() > 0){[m
[32m+[m[32m            return $package;[m
[32m+[m[32m        }else{[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
