[32m+[m[41m        [m
[32m+[m[32m        $per_page = 4;[m
[32m+[m[41m        [m
[32m+[m[32m        $page = Input::get('page',0);[m
[32m+[m[41m        [m
[32m+[m[32m        $rows = Member::all();[m
[32m+[m[41m        [m
[32m+[m[32m        $total_pages = ceil($rows->count()/$per_page);[m
[32m+[m[41m        [m
[32m+[m[32m        $offset = ($page == 0)? null : ($page - 1) * $per_page;[m
[32m+[m[41m        [m
[32m+[m[32m        $data = Member::orderBy('last_name')->take($per_page)->skip($offset)->get();[m
[32m+[m
[32m+[m[32m        $array = array([m
[32m+[m[32m            'page' => $page,[m
[32m+[m[32m            'total_items' => $rows->count(),[m
[32m+[m[32m            'total_pages' => $total_pages,[m
[32m+[m[32m            'total_showing' => count($data),[m
[32m+[m[32m            'members' => $data->toArray(),[m
[32m+[m[32m        );[m
[32m+[m[32m        $response = Response::make(json_encode($array,JSON_NUMERIC_CHECK),200);[m
[32m+[m[32m        $response->header('Content-Type', 'text/json');[m
[32m+[m[32m        return $response;[m
[32m+[m[41m         [m
[32m+[m[32m    });[m
[32m+[m[41m       [m
[32m+[m[32m});[m
