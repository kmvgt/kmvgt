[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Application & Route Filters[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| Below you will find the "before" and "after" events for the application[m
[32m+[m[32m| which may be used to do any work before or after a request into your[m
[32m+[m[32m| application. Here you may also register your custom route filters.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mApp::before(function($request)[m
[32m+[m[32m{[m
[32m+[m	[32m//[m
[32m+[m[32m    if(DB::table('members_packages')->whereRaw("UNIX_TIMESTAMP(expiration) <= ".strtotime("NOW"))->count() > 0){[m
[32m+[m[32m        DB::table('members_packages')->whereRaw("UNIX_TIMESTAMP(expiration) <= ".strtotime("NOW"))->update(array('status' => 0));[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m[32mApp::after(function($request, $response)[m
[32m+[m[32m{[m
[32m+[m	[32m//[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Authentication Filters[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| The following filters are used to verify that the user of the current[m
[32m+[m[32m| session is logged into this application. The "basic" filter easily[m
[32m+[m[32m| integrates HTTP Basic authentication for quick, simple checking.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mRoute::filter('auth', function()[m
[32m+[m[32m{[m
[32m+[m	[32mif (Auth::guest()) return Redirect::guest('login');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
[32m+[m[32mRoute::filter('auth.basic', function()[m
[32m+[m[32m{[m
[32m+[m	[32mreturn Auth::basic();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| Guest Filter[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| The "guest" filter is the counterpart of the authentication filters as[m
[32m+[m[32m| it simply checks that the current user is not logged in. A redirect[m
[32m+[m[32m| response will be issued if they are, which you may freely change.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mRoute::filter('guest', function()[m
[32m+[m[32m{[m
[32m+[m	[32mif (Auth::check()) return Redirect::to('/');[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m| CSRF Protection Filter[m
[32m+[m[32m|--------------------------------------------------------------------------[m
[32m+[m[32m|[m
[32m+[m[32m| The CSRF filter is responsible for protecting your application against[m
[32m+[m[32m| cross-site request forgery attacks. If this special token in a user[m
[32m+[m[32m| session does not match the one given in this request, we'll bail.[m
[32m+[m[32m|[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32mRoute::filter('csrf', function()[m
[32m+[m[32m{[m
[32m+[m	[32mif (Session::token() != Input::get('_token'))[m
[32m+[m	[32m{[m
[32m+[m		[32mthrow new Illuminate\Session\TokenMismatchException;[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mRoute::filter('csrf_header', function()[m
[32m+[m[32m{[m
[32m+[m[32m  if (Session::token() != Request::header('x-csrf-token'))[m
[32m+[m	[32m{[m
[32m+[m		[32mthrow new Illuminate\Session\TokenMismatchException;[m
[32m+[m	[32m}[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mRoute::filter('superadmin', function()[m
[32m+[m[32m{[m
[32m+[m[32m    if (!Sentry::check())[m
[32m+[m[32m    {[m
[32m+[m[32m        // User is not logged in, or is not activated[m
[32m+[m[32m        return Redirect::to('login');[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m});[m
