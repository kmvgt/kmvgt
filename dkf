[1mdiff --git a/PHPLaravelGymManagementSystem/app/controllers/AuthController.php b/PHPLaravelGymManagementSystem/app/controllers/AuthController.php[m
[1mnew file mode 100644[m
[1mindex 0000000..ea53492[m
[1m--- /dev/null[m
[1m+++ b/PHPLaravelGymManagementSystem/app/controllers/AuthController.php[m
[36m@@ -0,0 +1,79 @@[m
[32m+[m[32m<?php[m
[32m+[m[32mclass AuthController extends BaseController {[m
[32m+[m[41m        [m
[32m+[m[32m    public function login(){[m
[32m+[m[41m        [m
[32m+[m[32m        if (Sentry::check())[m
[32m+[m[32m        {[m
[32m+[m[32m            // User is not logged in, or is not activated[m
[32m+[m[32m            return Redirect::to('/');[m
[32m+[m[32m        }[m
[32m+[m[32m        return View::make('login',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function login_post(){[m
[32m+[m[41m       [m
[32m+[m[32m       if(!Request::ajax()){[m
[32m+[m[41m            [m
[32m+[m[32m            App::abort('401');[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $data = array([m
[32m+[m[32m            'status'    =>    'success',[m
[32m+[m[32m            'message'   =>     ''[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            // Set login credentials[m
[32m+[m[32m            $credentials = array([m
[32m+[m[32m                'email'    => Input::get('email'),[m
[32m+[m[32m                'password' => Input::get('password'),[m
[32m+[m[32m            );[m
[32m+[m[41m            [m
[32m+[m[32m            $remember = (Input::get('remember'))?Input::get('remember'):false;[m
[32m+[m[41m        [m
[32m+[m[32m            // Try to authenticate the user[m
[32m+[m[32m            $user = Sentry::authenticate($credentials, $remember);[m
[32m+[m[32m            $data['status'] = 'success';[m
[32m+[m[32m            $data['message'] = 'Login Success. Redirecting';[m
[32m+[m[41m            [m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Cartalyst\Sentry\Users\LoginRequiredException $e)[m
[32m+[m[32m        {[m
[32m+[m[32m            $data['status'] = 'error';[m
[32m+[m[32m            $data['message'] = 'Login field is required.';[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Cartalyst\Sentry\Users\PasswordRequiredException $e)[m
[32m+[m[32m        {[m
[32m+[m[32m            $data['status'] = 'error';[m
[32m+[m[32m            $data['message'] = 'Password field is required.';[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Cartalyst\Sentry\Users\WrongPasswordException $e)[m
[32m+[m[32m        {[m
[32m+[m[32m            $data['status'] = 'error';[m
[32m+[m[32m            $data['message'] = 'Wrong password, try again.';[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Cartalyst\Sentry\Users\UserNotFoundException $e)[m
[32m+[m[32m        {[m
[32m+[m[32m            $data['status'] = 'error';[m
[32m+[m[32m            $data['message'] = 'User was not found.';[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Cartalyst\Sentry\Users\UserNotActivatedException $e)[m
[32m+[m[32m        {[m
[32m+[m[32m            $data['status'] = 'error';[m
[32m+[m[32m            $data['message'] = 'User is not activated.';[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $response = Response::make(json_encode($data),200);[m
[32m+[m[32m        $response->header('Content-Type', 'text/json');[m
[32m+[m[32m        return $response;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function logout()[m
[32m+[m[32m    {[m
[32m+[m[32m        Sentry::logout();[m
[32m+[m[41m        [m
[32m+[m[32m        return Redirect::to('auth/login')->with('success','Successfully logged out.');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
