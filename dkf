[32m+[m[32m    public function settings(){[m
[32m+[m[41m        [m
[32m+[m[41m        [m
[32m+[m[32m        $this->data['settings'] =  Setting::orderBy('name')->get();[m
[32m+[m[32m        return $this->layout->content = View::make('settings',$this->data);[m
[32m+[m[41m        [m
