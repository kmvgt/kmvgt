[32m+[m[41m    [m
[32m+[m[32m    public function members(){[m
[32m+[m[32m        return $this->layout->content = View::make('members',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function search()[m
[32m+[m[32m    {[m
[32m+[m[32m        $keyword = Input::get('keyword');[m
[32m+[m[32m        // REDIRECT BACK TO HOMEPAGE IF EMPTY[m
[32m+[m[32m        if(empty($keyword)){[m
[32m+[m[32m            return Redirect::to('/');[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $this->data['members'] = Member::where('last_name','LIKE',"%$keyword%")[m
[32m+[m[32m                            ->orWhere('first_name','LIKE',"%$keyword%")[m
[32m+[m[32m                            ->orWhere('number','LIKE',"%$keyword%")[m
[32m+[m[32m                            ->orderBy('last_name')[m
[32m+[m[32m                            ->get();[m
[32m+[m[41m        [m
[32m+[m[32m        return $this->layout->content = View::make('members_search',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[32m    public function login($member_id)[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $member = Member::find($member_id);[m
[32m+[m[41m        [m
[32m+[m[32m        if(!empty($member) && is_object($member))[m
[32m+[m[32m        {[m
[32m+[m[32m            if($member->memberpackages->count() > 0){[m
[32m+[m[32m                $activity = new Activity;[m
[32m+[m[32m                $activity->member_id = $member_id;[m
[32m+[m[32m                $activity->timestamp = date("Y-m-d H:i:s");[m
[32m+[m[32m                $activity->save();[m
[32m+[m[32m                return Redirect::to('/');[m
[32m+[m[32m            }else{[m
[32m+[m[32m                return Redirect::to('/')->with('error','Member doesnt have any package.');[m
[32m+[m[32m            }[m
[32m+[m[32m        }else{ return Redirect::to('/'); }[m
[32m+[m[32m    }[m
[32m+[m[32m    public function create(){[m
[32m+[m[32m        return $this->layout->content = View::make('members_create',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[32m    public function create_post(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $rules = array([m
[32m+[m[32m            'first_name' => 'required',[m
[32m+[m[32m            'last_name' => 'required',[m
[32m+[m[32m            'email'     => 'email|unique:members,email',[m
[32m+[m[32m            'photo' => 'mimes:jpeg,bmp,png|max:200',[m
[32m+[m[32m            'dob' => 'date_format:Y-m-d',[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        $validator = Validator::make(Input::all(),$rules);[m
[32m+[m[41m        [m
[32m+[m[32m        if($validator->fails()){[m
[32m+[m[41m            [m
[32m+[m[32m            return Redirect::to('members/create')->withInput()->withErrors($validator);[m
[32m+[m[41m            [m
[32m+[m[32m        }else{[m
[32m+[m[32m            $filename = '';[m
[32m+[m[32m            if(Input::hasFile('photo')){[m
[32m+[m[32m                $file = Input::file('photo');[m[41m             [m
[32m+[m[32m                $filename = Input::file('photo')->getClientOriginalName();[m
[32m+[m[32m                $extension = substr(strrchr($filename,'.'),1);[m
[32m+[m[32m                $newname = strtolower(md5(date("Y-m-d").Input::get('username')).'.'.$extension);[m
[32m+[m[32m                $uploadStatus = Input::file('photo')->move(Config::get('app.upload_path'),$newname);[m
[32m+[m[32m                if($uploadStatus){ $filename = $newname; }[m[41m [m
[32m+[m[41m       [m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            $member = new Member;[m
[32m+[m[32m            $member->number = date("YmdHis");[m
[32m+[m[32m            $member->first_name = Input::get('first_name');[m
[32m+[m[32m            $member->last_name = Input::get('last_name');[m
[32m+[m[32m            $member->address = Input::get('address');[m
[32m+[m[32m            $member->phone = Input::get('phone');[m
[32m+[m[32m            $member->dob = Input::get('dob');[m
[32m+[m[32m            $member->email = Input::get('email');[m
[32m+[m[32m            $member->photo = $filename;[m
[32m+[m[32m            $member->save();[m
[32m+[m[41m            [m
[32m+[m[32m            return Redirect::to('members/create')->with('success','Member has been created.');[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m    }[m
[32m+[m[32m    public function update(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->data['member'] = Member::find(Request::segment(3));[m
[32m+[m[41m        [m
[32m+[m[32m        if(empty($this->data['member'])){[m
[32m+[m[32m            return Redirect::to('members')->with('error','Member was not found.');[m
[32m+[m[32m        }[m
[32m+[m[32m        //print_r($this->data['member'] );[m
[32m+[m[32m        return $this->layout->content = View::make('members_update',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[32m    public function update_post(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->data['member'] = Member::find(Request::segment(3));[m
[32m+[m[41m        [m
[32m+[m[32m        if(empty($this->data['member'])){[m
[32m+[m[32m            return Redirect::to('members')->with('error','Member was not found.');[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $rules = array([m
[32m+[m[32m            'first_name' => 'required',[m
[32m+[m[32m            'last_name' => 'required',[m
[32m+[m[32m            'email'     => 'email|unique:members,email,'.$this->data['member']->id,[m
[32m+[m[32m            'photo' => 'mimes:jpeg,bmp,png|max:200',[m
[32m+[m[32m            'dob' => 'date_format:Y-m-d',[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        $validator = Validator::make(Input::all(),$rules);[m
[32m+[m[41m        [m
[32m+[m[32m        if($validator->fails()){[m
[32m+[m[41m            [m
[32m+[m[32m            return Redirect::to('members/create')->withInput()->withErrors($validator);[m
[32m+[m[41m            [m
[32m+[m[32m        }else{[m
[32m+[m[32m            $filename = $this->data['member']->photo;[m
[32m+[m[32m            if(Input::hasFile('photo')){[m
[32m+[m[32m                $file = Input::file('photo');[m[41m             [m
[32m+[m[32m                $filename = Input::file('photo')->getClientOriginalName();[m
[32m+[m[32m                $extension = substr(strrchr($filename,'.'),1);[m
[32m+[m[32m                $newname = strtolower(md5(date("Y-m-d").Input::get('username')).'.'.$extension);[m
[32m+[m[32m                $uploadStatus = Input::file('photo')->move(Config::get('app.upload_path'),$newname);[m
[32m+[m[32m                if($uploadStatus){ $filename = $newname; }[m[41m [m
[32m+[m[41m       [m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            $member = Member::find(Request::segment(3));[m
[32m+[m[32m            $member->number = strtotime(date("Y-m-d-H:i:s"));[m
[32m+[m[32m            $member->first_name = Input::get('first_name');[m
[32m+[m[32m            $member->last_name = Input::get('last_name');[m
[32m+[m[32m            $member->address = Input::get('address');[m
[32m+[m[32m            $member->phone = Input::get('phone');[m
[32m+[m[32m            $member->dob = Input::get('dob');[m
[32m+[m[32m            $member->email = Input::get('email');[m
[32m+[m[32m            $member->photo = $filename;[m
[32m+[m[32m            $member->save();[m
[32m+[m[41m            [m
[32m+[m[32m            return Redirect::to('members')->with('success','Member has been updated.');[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function packages($member_id)[m
[32m+[m[32m    {[m
[32m+[m[32m        $member = Member::find($member_id);[m
[32m+[m[32m        if(empty($member) || !is_object($member)){[m
[32m+[m[32m            return Redirect::to('members')->with("error","Member doesnt exists.");[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $packages = Memberpackage::where('member_id','=',$member->id)->paginate(15);[m
[32m+[m[32m        $this->data['member'] = $member;[m
[32m+[m[32m        $this->data['packages'] = $packages;[m
[32m+[m[41m        [m
[32m+[m[32m        return $this->layout->content = View::make('members_packages',$this->data);[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function attach()[m
[32m+[m[32m    {[m
[32m+[m[32m        $member_id = Request::segment(3);[m
[32m+[m[41m        [m
[32m+[m[32m        $member = Member::find($member_id);[m
[32m+[m[32m        if(empty($member) || !is_object($member)){[m
[32m+[m[32m            return Redirect::to('members')->with("error","Member doesnt exists.");[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->data['packages'] = Package::all();[m
[32m+[m[32m        if(Package::count() == 0){[m
[32m+[m[32m            return Redirect::to('members/packages/'.$member_id)->with("error","No Available Packages.");[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->data['member'] = $member;[m
[32m+[m[32m        return $this->layout->content = View::make('members_packages_attach',$this->data);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function attach_post()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $member_id = Request::segment(3);[m
[32m+[m[41m        [m
[32m+[m[32m        $package_id = Input::get('package_id');[m
[32m+[m[41m        [m
[32m+[m[32m        $package = Package::find($package_id);[m
[32m+[m[41m        [m
[32m+[m[32m        $member = Member::find($member_id);[m
[32m+[m[41m        [m
[32m+[m[32m        if(empty($member) || !is_object($member)){[m
[32m+[m[32m            return Redirect::to('members')->with("error","Member doesnt exists.");[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $rules = array('package_id' => 'required|numeric');[m
[32m+[m[41m        [m
[32m+[m[32m        $validator = Validator::make(Input::all(),$rules);[m
[32m+[m[41m        [m
[32m+[m[32m        if($validator->fails()){[m
[32m+[m[32m            return Redirect::to('members/packages/'.$member_id.'/attach')->withErros($validator);[m
[32m+[m[32m        }else{[m
[32m+[m[32m            if(empty($package)){[m
[32m+[m[32m                return Redirect::to('members/packages/'.$member_id)->with('error','Package doenst exists');[m
[32m+[m[32m            }else{[m
[32m+[m[32m                // IF MEMBER IS STILL HAVING THE SAME KIND OF PACKAGE[m
[32m+[m[32m                if(Memberpackage::still_has($member_id,$package_id) ==  false)[m
[32m+[m[32m                {[m
[32m+[m[32m                    $member_package = new Memberpackage;[m
[32m+[m[32m                    $member_package->member_id = $member_id;[m
[32m+[m[32m                    $member_package->package_id = $package_id;[m
[32m+[m[41m                    [m
[32m+[m[32m                    // SET THE EXPIRATION DATE[m
[32m+[m[32m                    if($package->cycle->name == 'monthly')[m
[32m+[m[32m                    {[m
[32m+[m[32m                        $expiration = strtotime(date("Y-m-d")) + (30 * 86400);[m
[32m+[m[32m                        $member_package->expiration = date('Y-m-d', $expiration);[m
[32m+[m[32m                    }[m
[32m+[m[41m                    [m
[32m+[m[32m                    if($package->cycle->name == 'annually')[m
[32m+[m[32m                    {[m
[32m+[m[32m                        $expiration = strtotime(date("Y-m-d")) + (365 * 86400);[m
[32m+[m[32m                        $member_package->expiration = date('Y-m-d',$expiration);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    // REGISTRATION DATE[m
[32m+[m[32m                    $member_package->registration = date("Y-m-d");[m
[32m+[m[32m                    $member_package->status = 1;[m
[32m+[m[32m                    $member_package->save();[m
[32m+[m[32m                    return Redirect::to('members/packages/'.$member_id)->with('success','Package has been attached to the member.');[m
[32m+[m[41m                    [m
[32m+[m[32m                }else{[m
[32m+[m[32m                    return Redirect::to('members/packages/'.$member_id.'/attach')->with('error','Sorry the package is still associated with the member.');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function suspend()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[32m        $memberpackage = Memberpackage::find(Input::get('package_id'));[m
[32m+[m[32m        if($memberpackage->status == 1){[m
[32m+[m[41m            [m
[32m+[m[32m            $memberpackage->status = 0;[m
[32m+[m[32m            $memberpackage->suspended_at = date("Y-m-d");[m
[32m+[m[32m            $memberpackage->save();[m
[32m+[m[32m            echo 1;[m
[32m+[m[41m            [m
[32m+[m[32m        }else{[m
[32m+[m[41m            [m
[32m+[m[32m            if($memberpackage->package->cycle->name == 'monthly')[m
[32m+[m[32m            {[m
[32m+[m[32m                $memberpackage->status = 1;[m
[32m+[m[32m                $expiration = strtotime("now") + (30 * 86400);[m
[32m+[m[32m                $memberpackage->expiration = date('Y-m-d', $expiration);[m
[32m+[m[32m                $memberpackage->save();[m
[32m+[m[32m            }[m
[32m+[m[41m            [m
[32m+[m[32m            if($memberpackage->package->cycle->name == 'annually')[m
[32m+[m[32m            {[m
[32m+[m[32m                $memberpackage->status = 1;[m
[32m+[m[32m                $expiration = strtotime("now") + (365 * 86400);[m
[32m+[m[32m                $memberpackage->expiration = date('Y-m-d',$expiration);[m
[32m+[m[32m                $memberpackage->save();[m
[32m+[m[32m            }[m
[32m+[m[32m            echo 1;[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function extend()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[32m        $member_package = Memberpackage::find(Input::get('package_id'));[m
[32m+[m[41m        [m
[32m+[m[32m        // SET THE EXPIRATION DATE[m
[32m+[m[32m        if($member_package->package->cycle->name == 'monthly')[m
[32m+[m[32m        {[m
[32m+[m[32m            $expiration = strtotime($member_package->expiration) + (30 * 86400);[m
[32m+[m[32m            $member_package->expiration = date('Y-m-d', $expiration);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        if($member_package->package->cycle->name == 'annually')[m
[32m+[m[32m        {[m
[32m+[m[32m            $expiration = strtotime($member_package->expiration) + (365 * 86400);[m
[32m+[m[32m            $member_package->expiration = date('Y-m-d',$expiration);[m
[32m+[m[32m        }[m
[32m+[m[32m        $member_package->save();[m
[32m+[m[32m        echo 1;[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function delete_package()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[32m        $package_id = Input::get('package_id');[m
[32m+[m[32m        $memberpackage = Memberpackage::find($package_id);[m
[32m+[m[32m        $memberpackage->delete();[m
[32m+[m[32m        echo 1;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
