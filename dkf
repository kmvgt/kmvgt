[1mdiff --git a/PHPLaravelGymManagementSystem/bootstrap/start.php b/PHPLaravelGymManagementSystem/bootstrap/start.php[m
[1mnew file mode 100644[m
[1mindex 0000000..9848f9b[m
[1m--- /dev/null[m
[1m+++ b/PHPLaravelGymManagementSystem/bootstrap/start.php[m
