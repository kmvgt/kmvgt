[32m+[m[41m    [m
[32m+[m[32m    public function settings_post(){[m
[32m+[m[41m        [m
[32m+[m[32m        $this->layout = '';[m
[32m+[m[41m        [m
[32m+[m[32m        $options = Input::all();[m
[32m+[m[41m        [m
[32m+[m[32m        $rules = array();[m
[32m+[m[41m        [m
[32m+[m[32m        foreach($options as $k => $v)[m
[32m+[m[32m        {[m
[32m+[m[32m            $r = Setting::rule($k);[m
[32m+[m[32m            if($r != false)[m
[32m+[m[32m            {[m
[32m+[m[32m                $rules[$k] = $r;[m
[32m+[m[32m            }[m[41m  [m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $validator = Validator::make(Input::all(),$rules);[m
[32m+[m[32m        if($validator->fails())[m
[32m+[m[32m        {[m
[32m+[m[32m            return Redirect::to('settings')->withErrors($validator);[m
[32m+[m[32m        }else{[m
[32m+[m[41m            [m
[32m+[m[32m            foreach($options as $k => $v)[m
[32m+[m[32m            {[m
[32m+[m[32m                DB::table('settings')->where('name',$k)->update(array('value' => $v));[m
[32m+[m[32m            }[m
[32m+[m[32m            return Redirect::to('settings')->with('success','Settings has been successfully updated.');[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    public function admin_post()[m
[32m+[m[32m    {[m
[32m+[m[32m        $rules = array([m
[32m+[m[32m            'email' => 'required|email'[m
[32m+[m[32m        );[m
[32m+[m[41m        [m
[32m+[m[32m        if(Input::has('password')){[m
[32m+[m[32m            $rules['password'] = 'confirmed|required|min:6:max:12';[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        $validator = Validator::make(Input::all(),$rules);[m
[32m+[m[41m        [m
[32m+[m[32m        if($validator->fails())[m
[32m+[m[32m        {[m
[32m+[m[32m            return Redirect::to('settings')->withErrors($validator)->withInput();[m
[32m+[m[32m        }else{[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                // Find the user using the user id[m
[32m+[m[32m                $user = Sentry::getUserProvider()->findById($this->data['admin']->id);[m
[32m+[m[41m            [m
[32m+[m[32m                // Update the user details[m
[32m+[m[32m                $user->email = Input::get('email');[m
[32m+[m[32m                $user->first_name = Input::get('password');[m
[32m+[m[41m            [m
[32m+[m[32m                // Update the user[m
[32m+[m[32m                if ($user->save())[m
[32m+[m[32m                {[m
[32m+[m[32m                   return Redirect::to('settings')->with('success','User information was updated.');[m[41m [m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    return Redirect::to('settings')->with('error','User information was not updated.');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Cartalyst\Sentry\Users\UserExistsException $e)[m
[32m+[m[32m            {[m
[32m+[m[32m                return Redirect::to('settings')->with('error','User with this login already exists.');[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Cartalyst\Sentry\Users\UserNotFoundException $e)[m
[32m+[m[32m            {[m
[32m+[m[32m                return Redirect::to('settings')->with('error','User was not found.');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
