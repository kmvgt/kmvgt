[32m+[m[41m    [m
[32m+[m[32m    public function main()[m
[32m+[m	[32m{[m
[32m+[m[32m         $this->data['activities'] = Activity::where(DB::raw("DATE(timestamp)"),'=',date("Y-m-d"))->orderBy('id','desc')->paginate(25);[m
[32m+[m[32m         $this->data['packages'] = Memberpackage::whereRaw("UNIX_TIMESTAMP(expiration) <= ".strtotime('NOW'))->take(15)->get();[m
[32m+[m[32m         $this->data['total_members'] = Member::count();[m
[32m+[m[32m         $this->data['total_packages'] = Package::count();[m
[32m+[m[32m         $this->data['total_services'] = Service::count();[m
[32m+[m[32m         return $this->layout->content = View::make('main',$this->data);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m}[m
